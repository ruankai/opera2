<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    
    <class name="com.org.opera.domain.Vip" table="VIP">
	
		<id name="id" type="java.lang.Long">
			<column name="id" length="10"></column>
			<generator class="native"></generator>
		</id>
	
		<property name="password" type="java.lang.String">
			<column name="password" length="32"></column>
		</property>
		
		<property name="name" type="java.lang.String">
			<column name="name" length="32"></column>
		</property>
		
		<property name="trueName" type="java.lang.String">
			<column name="trueName" length="32"></column>
		</property>
		
		<property name="gender" type="java.lang.String">
			<column name="gender" length="32"></column>
		</property>
		
		<property name="birthday" type="java.util.Date">
			<column name="birthday"></column>
		</property>
		
		<property name="place" type="java.lang.String">
			<column name="place" length="32"></column>
		</property>
		
		<property name="tel" type="java.lang.String">
			<column name="tel" length="32"></column>
		</property>
		
		<property name="email" type="java.lang.String">
			<column name="email" length="32"></column>
		</property>
		
		<property name="wechat" type="java.lang.String">
			<column name="wechat" length="32"></column>
		</property>
		
		<property name="qq" type="java.lang.String">
			<column name="qq" length="32"></column>
		</property>
		
		<property name="organizationId" type="java.lang.String">
			<column name="organizationId" length="32"></column>
		</property>
		
		<property name="companyTel" type="java.lang.String">
			<column name="companyTel" length="32"></column>
		</property>
		
		<property name="companyPlace" type="java.lang.String">
			<column name="companyPlace" length="32"></column>
		</property>
		
		<property name="duty" type="java.lang.String">
			<column name="duty" length="32"></column>
		</property>
		
		<property name="rank" type="java.lang.String">
			<column name="rank" length="32"></column>
		</property>
		
		<property name="school" type="java.lang.String">
			<column name="school" length="32"></column>
		</property>
		
		<property name="tenureClock" type="java.lang.String">
			<column name="tenureClock" length="32"></column>
		</property>
		
		<property name="education" type="java.lang.String">
			<column name="education" length="32"></column>
		</property>
		
		<property name="degree" type="java.lang.String">
			<column name="degree" length="32"></column>
		</property>
		
		<property name="pose" type="java.lang.String">
			<column name="pose" length="32"></column>
		</property>
		
		<property name="remark" type="java.lang.String">
			<column name="remark" length="32"></column>
		</property>
		
		<property name="type" type="java.lang.String">
			<column name="type" length="32"></column>
		</property>
		
	</class>
    
</hibernate-mapping>